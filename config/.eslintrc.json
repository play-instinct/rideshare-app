{
    "extends": "airbnb-base",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "max-len": "off",
        "no-underscore-dangle": ["error", { "allow": ["_message", "_id"]}]
    }
}